<p style="color: green"><%= notice %></p>

<% content_for :title, "Forms" %>

<h1>Forms</h1>

<div id="forms">
  <% @forms.each do |form| %>
    <%= render form %>
    <p>
      <%= link_to "Show", form %> |
      <%= link_to "Fill", new_form_submission_path(form) %> |
      <%= link_to "Submissions", form_submissions_path(form) %>
    </p>
  <% end %>
</div>

<%= link_to "New form", new_form_path %>
